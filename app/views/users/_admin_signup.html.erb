      <!-- END: Left Aside -->
      <div class="m-grid__item m-grid__item--fluid m-wrapper">
        <!-- BEGIN: Subheader -->
        <div class="m-subheader ">
          <div class="d-flex align-items-center">
            <div class="mr-auto">
              <h3 class="m-subheader__title m-subheader__title--separator">
                Add New User
              </h3>
              <ul class="m-subheader__breadcrumbs m-nav m-nav--inline">
                <li class="m-nav__item m-nav__item--home">
                  <a href="<%= root_url %>" class="m-nav__link m-nav__link--icon">
                    <i class="m-nav__link-icon la la-home"></i>
                  </a>
                </li>
                <li class="m-nav__separator">
                  -
                </li>
                <li class="m-nav__item">
                  <a href="<%= users_url %>" class="m-nav__link">
                                    <span class="m-nav__link-text">
                                        Users List
                                    </span>
                  </a>
                </li>
                <li class="m-nav__separator">
                  -
                </li>
                <li class="m-nav__item active">
                  <a href="<%= signup_url %>" class="m-nav__link">
                                    <span class="m-nav__link-text">
                                        Add User
                                    </span>
                  </a>
                </li>

              </ul>
            </div>

          </div>
        </div>
        <!-- END: Subheader -->
            <div class="m-content">
              <!--begin::Portlet-->
              <div class="m-portlet">
                <div class="m-portlet__head">
                  <div class="m-portlet__head-caption">
                    <div class="m-portlet__head-title">
                      <h3 class="m-portlet__head-text">
                        New User
                      </h3>
                    </div>
                  </div>
                </div>
                <!--begin::Form-->
                <%= form_with model: @user, html: { class: 'm-form m-form--fit m-form--label-align-right' }, local: true do |form| %>
<!--                <form class="m-form m-form--fit m-form--label-align-right" id="m_form_1">-->

                  <div class="m-portlet__body">

                      <div class="m-alert m-alert--icon alert alert-danger m--hide" role="alert" id="m_form_1_msg">
                        <div class="m-alert__icon">
                          <i class="la la-warning"></i>
                        </div>
                        <div class="m-alert__text">
                          Oh snap! Change a few things up and try submitting again.
                        </div>
                        <div class="m-alert__close">
                          <button type="button" class="close" data-close="alert" aria-label="Close"></button>
                        </div>
                      </div>
                    </div>

                  <div class="m-form__content">
                    <% if @user.errors.any? %>
                      <div id="error_explanation">


                        <% @user.errors.full_messages.each do |msg| %>
                          <div>
                            <p class = "text text-danger">&nbsp&nbsp&nbsp&nbsp&nbsp
                              &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp
                              &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp
                              &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp
                              *&nbsp<%= msg %></p></div>
                        <% end %>
                      </div>
                      </div>
                    <% end %>

                    <div class="form-group m-form__group row">
                      <label class="col-form-label col-lg-3 col-sm-12">
                        Name *
                      </label>
                      <div class="col-lg-4 col-md-9 col-sm-12">
                        <%= form.text_field :name, class: 'form-control m-input', placeholder: 'Enter your Name', required: true %>
<!--                        <input type="text" class="form-control m-input" name="user[name]" placeholder="Enter your Name" >-->
                      </div>
                    </div>
                    <div class="form-group m-form__group row">
                      <label class="col-form-label col-lg-3 col-sm-12">
                        Email *
                      </label>
                      <div class="col-lg-4 col-md-9 col-sm-12">
                        <%= form.text_field :email, class: 'form-control m-input', placeholder: 'Enter your Email', required: true %>
<!--                        <input type="text" class="form-control m-input" name="user[email]" placeholder="Enter your email" >-->
                      </div>
                    </div>
                    <div class="form-group m-form__group row">
                      <label class="col-form-label col-lg-3 col-sm-12">
                        Password *
                      </label>
                      <div class="col-lg-4 col-md-9 col-sm-12">
                        <%= form.password_field :password, class: 'form-control m-input', placeholder: 'Enter your Password', required: true %>
<!--                        <input type="password" class="form-control m-input" name="user[password]" placeholder="Enter your Password" >-->
                      </div>
                    </div>
                    <div class="form-group m-form__group row">
                      <label class="col-form-label col-lg-3 col-sm-12">
                        Confirm Password *
                      </label>
                      <div class="col-lg-4 col-md-9 col-sm-12">
                        <%= form.password_field :password_confirmation, class: 'form-control m-input', placeholder: 'Confirm your Password', required: true %><br>

                        <!--                        <input type="password" class="form-control m-input" name="user[password_confirmation]" placeholder="Enter your Confirm Password" >-->
                      </div>
                    </div>

                    <div class="form-group m-form__group row">
                      <label class="col-form-label col-lg-3 col-sm-12">
                        User Type *
                      </label>
                      <div class="col-lg-4 col-md-9 col-sm-12">
                        <%= form.select(:role, User.roles.keys.map {|role| [role.titleize,role]}) %>
                        <!--                        <input type="password" class="form-control m-input" name="user[password_confirmation]" placeholder="Enter your Confirm Password" >-->
                      </div>
                    </div>

                    <div class="form-group m-form__group row">
                      <label class="col-form-label col-lg-3 col-sm-12">
                        Avatar *
                      </label>
                      <div class="col-lg-4 col-md-9 col-sm-12">
                        <%=javascript_include_tag 'image.change' %>
                        <%= form.file_field :avatar, onchange: 'validateFiles(this);',data: { max_file_size: 5.megabytes }, required: true %>
                      <br>
                      </div>
                    </div>

                  </div>
                  <div class="m-portlet__foot m-portlet__foot--fit">
                    <div class="m-form__actions m-form__actions">
                      <div class="row">
                        <div class="col-lg-9 ml-lg-auto">
                          <button type="submit" name="commit" class="btn btn-success">
                            Submit
                          </button>
                          <a href="<%=users_url %>" button type="reset" class="btn btn-secondary">
                            Cancel</a>
                          </button>
                        </div>
                      </div>
                    </div>
                  </div>

                </form>
                  <%end %>
                <!--end::Form-->
              </div>
              <!--end::Portlet-->
            </div>

        <!-- end:: Body -->

        <!-- END EXAMPLE TABLE PORTLET-->
        </div>
      </div>
    </div>
    <!-- end:: Body -->
    <!-- begin::Footer -->

    <!-- end::Footer -->
  </div>
  <!-- end:: Page -->
